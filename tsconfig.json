{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "declaration": false,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["**/*.ts", "webpack.config.js"],
  "exclude": ["node_modules"]
}
